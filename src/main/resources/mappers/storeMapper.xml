<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hodoos.test.database.dao.StoreDAO">
	
	<select id="selectStoreList" resultType="com.hodoos.test.database.model.Store">
		SELECT
			`id`
			, `phoneNumber`
			, `address`
			, `businessNumber`
			, `introduce`
			, `createdAt`
			, `updatedAt`
		FROM
			`new_store`
	
	</select>

</mapper>